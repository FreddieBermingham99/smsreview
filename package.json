{
  "name": "stasher-review-sms",
  "version": "1.0.0",
  "description": "Daily SMS service to request Google reviews from Stasher customers",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "migrate": "node dist/migrate.js",
    "job": "RUN_JOB=true node dist/index.js",
    "job:dry-run": "DRY_RUN=true RUN_JOB=true node dist/index.js"
  },
  "keywords": [
    "sms",
    "review",
    "stasher"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "libphonenumber-js": "^1.11.0",
    "luxon": "^3.4.4",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/csv-parse": "^1.1.12",
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.7.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
